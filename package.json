{
  "name": "edge-orchestrator-ui",
  "version": "3.0.0-dev",
  "description": "Edge Orchestrator User Interface",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run app:admin\" \"npm run app:infra\" \"npm run app:cluster-orch\" \"npm run app:app-orch\" \"npm run app:root\"",
    "app:admin": "cd apps/admin && webpack serve --config=config/webpack.dev.js",
    "app:admin:build": "cd apps/admin && webpack --config=config/webpack.prod.js",
    "app:admin:lint": "cd apps/admin && prettier --check ./src && eslint ./src",
    "app:admin:fix": "cd apps/admin && prettier --write ./src && eslint --fix ./src",
    "app:admin:cy:component": "cd tests/admin && cypress run --component --spec ../../apps/admin/unit-tests.cy.ts",
    "app:infra": "cd apps/infra && webpack serve --config=config/webpack.dev.js",
    "app:infra:build": "cd apps/infra && webpack --config=config/webpack.prod.js",
    "app:infra:lint": "cd apps/infra && prettier --check ./src && eslint ./src",
    "app:infra:fix": "cd apps/infra && prettier --write ./src && eslint --fix ./src",
    "app:infra:cy:component": "cd tests/infra && TZ=\"Asia/Kolkata\" cypress run --component --spec ../../apps/infra/unit-tests.cy.ts",
    "app:app-orch": "cd apps/app-orch && webpack serve --config=config/webpack.dev.js",
    "app:app-orch:build": "cd apps/app-orch && webpack --config=config/webpack.prod.js",
    "app:app-orch:lint": "cd apps/app-orch && prettier --check ./src && eslint ./src",
    "app:app-orch:fix": "cd apps/app-orch && prettier --write ./src && eslint --fix ./src",
    "app:app-orch:cy:component": "cd tests/app-orch && cypress run --component --spec ../../apps/app-orch/unit-tests.cy.ts",
    "app:root": "cd apps/root && webpack serve --config=config/webpack.dev.js",
    "app:root:build": "cd apps/root && webpack --config=config/webpack.prod.js",
    "app:root:lint": "cd apps/root && prettier --check ./src && eslint ./src",
    "app:root:fix": "cd apps/root && prettier --write ./src && eslint --fix ./src",
    "app:root:cy:component": "cd tests/root && cypress run --component --spec ../../apps/root/unit-tests.cy.ts",
    "app:cluster-orch": "cd apps/cluster-orch && webpack serve --config=config/webpack.dev.js",
    "app:cluster-orch:build": "cd apps/cluster-orch && webpack --config=config/webpack.prod.js",
    "app:cluster-orch:lint": "cd apps/cluster-orch && prettier --check ./src && eslint ./src",
    "app:cluster-orch:fix": "cd apps/cluster-orch && prettier --write ./src && eslint --fix ./src",
    "app:cluster-orch:cy:component": "cd tests/cluster-orch && cypress run --component --spec ../../apps/cluster-orch/unit-tests.cy.ts",
    "cypress:component": "cd tests && cypress open --component",
    "library:lint": "prettier --check ./library && eslint ./library",
    "library:fix": "cd library && prettier --write ./ && eslint --fix ./",
    "library:cy:component": "cd tests/library && cypress run --component --spec ./unit-tests.cy.ts",
    "tests:component": "cd tests && cypress open --component",
    "tests:e2e": "cd tests && cypress open --e2e --env EN_UUID=398395da-c10e-7c47-9b4f-efb34c0b261e",
    "tests:build": "cd tests && webpack build --config webpack.config.js",
    "api:generate": "bash ./library/apis/generate-api.sh --prefix library/apis/",
    "prettify": "npm run app:admin:fix & npm run app:infra:fix & npm run app:cluster-orch:fix & npm run app:app-orch:fix & npm run app:root:fix"
  },
  "author": "Edge Orchestrator UI Team",
  "license": "Apache-2.0",
  "devDependencies": {
    "@cypress/code-coverage": "^3.13.10",
    "@jsdevtools/coverage-istanbul-loader": "^3.0.5",
    "@pactflow/pact-cypress-adapter": "^1.3.0",
    "@rtk-query/codegen-openapi": "^1.2.0",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/prismjs": "^1.26.5",
    "@types/react-dom": "^18.0.10",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-table": "^7.7.20",
    "@types/react-transition-group": "^4.4.12",
    "concurrently": "^9.1.2",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "cypress": "^13.17.0",
    "cypress-keycloak": "^2.0.2",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-terminal-report": "^7.1.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-cypress": "^2.15.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.4.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-no-unsanitized": "^4.1.2",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-promise": "^6.6.0",
    "eslint-plugin-react": "^7.37.3",
    "headers-polyfill": "^3.2.5",
    "html-webpack-plugin": "^5.6.3",
    "ignore-loader": "^0.1.2",
    "js-yaml": "^4.1.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "msw": "^1.0.0",
    "path-browserify": "^1.0.1",
    "prettier": "^3.2.5",
    "prettier-plugin-organize-imports": "^3.2.4",
    "react-dev-utils": "^12.0.1",
    "sass": "^1.83.1",
    "sass-loader": "^16.0.4",
    "serve": "^14.2.4",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typescript": "^4.9.4",
    "webpack": "^5.97.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "webpack-remove-code-blocks": "^0.10.1"
  },
  "dependencies": {
    "@react-aria/utils": "^3.14.2",
    "@reduxjs/toolkit": "^2.5.0",
    "@spark-design/brand-tokens": "file:library/@spark-design/brand-tokens",
    "@spark-design/core": "file:library/@spark-design/core",
    "@spark-design/css": "file:library/@spark-design/css",
    "@spark-design/iconfont": "file:library/@spark-design/iconfont",
    "@spark-design/react": "file:library/@spark-design/react",
    "@spark-design/tokens": "file:library/@spark-design/tokens",
    "@spark-design/utils": "file:library/@spark-design/utils",
    "axios": "^1.8.2",
    "echarts": "^5.4.3",
    "jss": "^10.9.2",
    "jss-preset-default": "^10.9.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "polished": "^4.2.2",
    "prismjs": "^1.30.0",
    "query-string": "^9.1.1",
    "react": "18.2.0",
    "react-aria": "^3.22.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.3",
    "react-error-boundary": "^4.0.13",
    "react-hook-form": "^7.54.2",
    "react-oidc-context": "^3.2.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "^6.22.2",
    "react-stately": "^3.20.0",
    "react-table": "^7.8.0",
    "react-transition-group": "^4.4.5",
    "redux": "^5.0.1"
  }
}
